{
  "expo": {
    "name": "App do Aluno",
    "slug": "app-aluno",
    "version": "0.7.0",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0057d6"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.puccampinas.appaluno"
    },
    "android": {
      "package": "com.puccampinas.appaluno"
    }
  }
}
